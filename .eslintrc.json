{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion":2021,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
        
    ],
    "rules": {
        "class-methods-use-this": 0,
        "import/prefer-default-export": 0,
        "no-tabs": 0,
        "no-mixed-spaces-and-tabs": 0,
        "import/extensions": 0,
        "@typescript-eslint/indent": [
            "error",
            2
        ]
    },
    "settings": {
        "import/resolver": {
            "typescript": {
            }
        }
        
    }
}
